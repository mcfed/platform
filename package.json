{
  "name": "user-center",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "lerna exec --scope @platform/app -- npm start",
    "build:type": "lerna run build",
    "test": "react-app-rewired test --watchAll=false",
    "test:watch": "react-app-rewired test",
    "coverage": "cross-env CI=true react-app-rewired test --coverage",
    "template": "mcfcra -r"
  },
  "devDependencies": {
    "@mcfed/cra": "0.2.2-rc2",
    "husky": "^2.7.0",
    "lint-staged": "8.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "react-app-rewired": "^2.1.6",
    "source-map-explorer": "^2.4.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "dist",
    "build",
    "README.md"
  ],
  "config": {
    "API_SERVER": "/",
    "MOCK_SERVER": "/mock"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@commitlint/config-conventional": "^9.1.2",
    "@mcfed/router": "^1.0.0-beta",
    "babel-plugin-import": "^1.13.0",
    "universal-router": "^9.0.1"
  }
}
