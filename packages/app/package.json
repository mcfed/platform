    "hard-source-webpack-plugin": "^0.13.1",
    "jest-fetch-mock": "^3.0.3",
    "less": "^2.0.0",
    "less-loader": "^5.0.0",
    "react-app-rewired": "^2.1.6",
    "source-map-explorer": "^2.4.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "dist",
    "build",
    "README.md"
