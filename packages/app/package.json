  "scripts": {
    "analyze": "source-map-explorer build/static/js/*.*",
    "start": "react-app-rewired start",
    "build:web": "react-app-rewired build",
    "build": "npm run build:cjs",
    "build:cjs": "tsc --noEmit false --module commonjs --outDir dist/cjs",
    "build:umd": "tsc --noEmit false --resolveJsonModule false  --module umd --outDir dist/umd",
    "test": "react-app-rewired test --watchAll=false",
    "test:watch": "react-app-rewired test",
    "coverage": "cross-env CI=true react-app-rewired test --coverage",
    "eject": "react-app-rewired eject",
    "electron": "cross-env ELECTRON_ENABLE_LOGGING=true && electron client.js --debug &",
    "template": "mcfcra -r newTemplate",
    "package": "node_modules/.bin/electron-packager build abc-ts  --electronVersion=1.7.5 --platform=darwin   --out=releases  --overwrite "
  },
  "devDependencies": {
    "@mcfed/cra": "0.2.0",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.5",
